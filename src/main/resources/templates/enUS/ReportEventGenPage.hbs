{{#*inline "htmScriptReportEventPage"}}		<!-- inline "htmScriptReportEventPage" -->
		<script>
			$(document).ready(function() {
				document.onkeydown = function(evt) {
					evt = evt || window.event;
					var isEscape = false;
					if ('key' in evt) {
						isEscape = (evt.key === 'Escape' || evt.key === 'Esc');
					} else {
						isEscape = (evt.keyCode === 27);
					}
					if (isEscape) {
						$('.w3-modal:visible').hide();
					}
				};
				window.eventBus = new EventBus('/eventbus');
				var pk = {{#if pk}}{{pk}}{{else}}null{{/if}};
				if(pk != null) {
		{{#ifContainsAnyRoles roles rolesRequired}}
					suggestReportEventScheduleKey([{'name':'fq','value':'eventKeys:' + pk}], $('#listReportEventScheduleKey_Page'), pk, true);
		{{else}}
					suggestReportEventScheduleKey([{'name':'fq','value':'eventKeys:' + pk}], $('#listReportEventScheduleKey_Page'), pk, false);
		{{/ifContainsAnyRoles}}
		{{#ifContainsAnyRoles roles rolesRequired}}
					suggestReportEventAssigneeKey([{'name':'fq','value':'eventKeys:' + pk}], $('#listReportEventAssigneeKey_Page'), pk, true);
		{{else}}
					suggestReportEventAssigneeKey([{'name':'fq','value':'eventKeys:' + pk}], $('#listReportEventAssigneeKey_Page'), pk, false);
		{{/ifContainsAnyRoles}}
				}
				websocketReportEvent(websocketReportEventInner);
			});
		</script>
